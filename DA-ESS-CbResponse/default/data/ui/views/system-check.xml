<form>
  <label>System Check</label>
  <fieldset submitButton="true">
    <input type="text" token="hostname">
      <label>Enter Computer Name</label>
    </input>
    <input type="time" token="time">
      <label></label>
      <default>
        <earliest>-24h@h</earliest>
        <latest>now</latest>
      </default>
    </input>
  </fieldset>
  <row>
    <panel>
      <single>
        <title>Feed Hits</title>
        <search>
          <query>`cb` computer_name=$hostname$ type=feed.storage.hit.* OR type=feed.ingress.hit.* | stats count</query>
          <earliest>$time.earliest$</earliest>
          <latest>$time.latest$</latest>
        </search>
        <option name="colorBy">value</option>
        <option name="colorMode">none</option>
        <option name="drilldown">none</option>
        <option name="height">100</option>
        <option name="numberPrecision">0</option>
        <option name="rangeColors">["0x65a637","0x6db7c6","0xf7bc38","0xf58f39","0xd93f3c"]</option>
        <option name="rangeValues">[0,30,70,100]</option>
        <option name="showSparkline">1</option>
        <option name="showTrendIndicator">1</option>
        <option name="trendColorInterpretation">standard</option>
        <option name="trendDisplayMode">absolute</option>
        <option name="unitPosition">after</option>
        <option name="useColors">0</option>
        <option name="useThousandSeparators">1</option>
      </single>
    </panel>
    <panel>
      <single>
        <title>Watchlist Hits</title>
        <search>
          <query>`cb` computer_name=$hostname$ type=watchlist.hit.* OR type=watchlist.storage.hit.*| stats count</query>
          <earliest>$time.earliest$</earliest>
          <latest>$time.latest$</latest>
        </search>
        <option name="colorBy">value</option>
        <option name="colorMode">none</option>
        <option name="drilldown">none</option>
        <option name="height">100</option>
        <option name="numberPrecision">0</option>
        <option name="rangeColors">["0x65a637","0x6db7c6","0xf7bc38","0xf58f39","0xd93f3c"]</option>
        <option name="rangeValues">[0,30,70,100]</option>
        <option name="showSparkline">1</option>
        <option name="showTrendIndicator">1</option>
        <option name="trendColorInterpretation">standard</option>
        <option name="trendDisplayMode">absolute</option>
        <option name="unitPosition">after</option>
        <option name="useColors">0</option>
        <option name="useThousandSeparators">1</option>
      </single>
    </panel>
  </row>
  <row>
    <panel>
      <table>
        <title>CommandLine by Process</title>
        <search>
          <query>`cb` computer_name=$hostname$ type="ingress.event.procstart" | stats values(command_line) by process_path</query>
          <earliest>$time.earliest$</earliest>
          <latest>$time.latest$</latest>
        </search>
        <option name="dataOverlayMode">none</option>
        <option name="drilldown">cell</option>
        <option name="percentagesRow">false</option>
        <option name="rowNumbers">false</option>
        <option name="totalsRow">false</option>
        <option name="wrap">true</option>
      </table>
    </panel>
  </row>
  <row>
    <panel>
      <table>
        <title>Domains by Remote Port</title>
        <search>
          <query>`cb` computer_name=$hostname$ type="ingress.event.netconn" | stats values(domain) by remote_port</query>
          <earliest>$time.earliest$</earliest>
          <latest>$time.latest$</latest>
        </search>
        <option name="dataOverlayMode">none</option>
        <option name="drilldown">cell</option>
        <option name="percentagesRow">false</option>
        <option name="rowNumbers">false</option>
        <option name="totalsRow">false</option>
        <option name="wrap">true</option>
      </table>
    </panel>
    <panel>
      <table>
        <title>Critical Process Check</title>
        <search>
          <query>`cb` computer_name=$hostname$ type="ingress.event.procstart" process_path="*\\powershell.exe" OR process_path="*\\msbuild.exe" OR process_path="*\\psexec.exe" OR process_path="*\\at.exe" OR process_path="*\\schtasks.exe" OR process_path="*\\net.exe" OR process_path="*\\vssadmin.exe" OR process_path="*\\utilman.exe" OR process_path="*\\wmic.exe" OR process_path="*\\mshta.exe" OR process_path="*\\wscript.exe" OR process_path="*\\cscript.exe" OR process_path="*\\cmd.exe" OR process_path="*\\whoami.exe" OR process_path="*\\mmc.exe" OR process_path="*\\systeminfo.exe" OR process_path="*\\csvde.exe" OR process_path="*\\certutil.exe" | stats count by process_path</query>
          <earliest>$time.earliest$</earliest>
          <latest>$time.latest$</latest>
        </search>
        <option name="dataOverlayMode">none</option>
        <option name="drilldown">cell</option>
        <option name="percentagesRow">false</option>
        <option name="rowNumbers">false</option>
        <option name="totalsRow">false</option>
        <option name="wrap">true</option>
      </table>
    </panel>
  </row>
</form>
